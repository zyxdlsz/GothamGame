<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="main.gotham">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".StaffActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".FoodActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitle">
        </activity>
        <activity android:name=".PeopleActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitle">
        </activity>
        <activity android:name=".SinglePActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitle">
        </activity>
        <activity android:name=".PeopleChatActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitle">
        </activity>
        <activity android:name=".CollectionActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitleFull">
        </activity>
        <activity android:name=".EquipActivity"
            android:theme="@style/Theme.AppCompat.Light.DialogNoTitleFull">
        </activity>
        <activity android:name=".TestActivity">
        </activity>
    </application>

</manifest>